public class Quizz {
	//LinkedList
	public static void main(String[] args) {
		//
		List<Integer> list = new LinkedList<>();
		Queue<Integer> queue = new LinkedList<>();
		LinkedList<Integer> ll = new LinkedList<>();
		
		for(int i=5; i>0;i--) {
			list.add(i);
			queue.add(i);
			ll.add(i);	
		}
		list.remove(2);
		queue.remove(2);
		ll.remove(2);
		
